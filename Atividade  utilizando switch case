let nota = prompt("Insira a nota (0-100): ");
let conceito;

switch (true) {
  case nota >= 90 && nota <= 100:
    conceito = "A";
    break;
  case nota >= 80 && nota <= 89:
    conceito = "B";
    break;
  case nota >= 70 && nota <= 79:
    conceito = "C";
    break;
  case nota >= 60 && nota <= 69:
    conceito = "D";
    break;
  case nota >= 0 && nota <= 59:
    conceito = "F";
    break;
  default:
    console.log("Nota invÃ¡lida");
}

if (conceito) {
  console.log(`Conceito: ${conceito}`);
}